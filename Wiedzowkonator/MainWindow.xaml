                <MenuItem Header="_Plik">
                    <MenuItem Header="_Otwórz" Click="Open"/>
                    <MenuItem Header="_Zapisz" Click="Save"/>
                    <MenuItem Header="_Wczytaj" Click="Load"/>
                    <MenuItem Header="_Usuń Końcówki" Click="DeleteEndings"/>
                </MenuItem>

            </Menu>
